{
  "skills": [
    "vercel-labs/skills@vercel-react-best-practices",
    "vercel-labs/skills@web-design-guidelines"
  ],
  "mcpServers": {
    "supabase": {
      "type": "stdio",
      "command": "npx",
      "args": ["@supabase/mcp-server-supabase"],
      "env": {
        "SUPABASE_ACCESS_TOKEN": "${SUPABASE_ACCESS_TOKEN}"
      }
    },
    "github": {
      "type": "stdio",
      "command": "npx",
      "args": ["@modelcontextprotocol/server-github"],
      "env": {
        "GITHUB_TOKEN": "${GITHUB_TOKEN}"
      }
    },
    "context7": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "@upstash/context7-mcp"],
      "env": {}
    },
    "sequential-thinking": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-sequential-thinking"],
      "env": {}
    },
    "desktop-commander": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "@anthropic/desktop-commander-mcp"],
      "env": {}
    },
    "stitch": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "@anthropic/stitch-mcp"],
      "env": {}
    }
  }
}
